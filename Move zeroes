class Solution {
public:
    void moveZeroes(vector<int>& nums) {
        int l=0;
        int n=nums.size();
        int i=0;
        while(i<n)
        {
          if(nums[i]!=0){
            int t=nums[l];
            nums[l]=nums[i];
            nums[i]=t;
            l++;
          }
          i++;
        }
    }
};
